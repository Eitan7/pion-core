# pion-platform configuration file for pkg-config

prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@
datarootdir=@datarootdir@
datadir=@datarootdir@

Name: pion-platform
Description: an open source platform for real-time event stream processing
Version: @VERSION@
#Requires: boost-1.34 boost-asio-0.3.8rc3
Libs: @LDFLAGS@ @PION_EXTERNAL_LIBS@ -L${libdir} -lpion-common -lpion-net -lpion-reactor -lpion-platform
Cflags: @CPPFLAGS@ -I${includedir}
