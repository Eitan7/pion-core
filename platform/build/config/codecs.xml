    <Field term="urn:vocab:clickstream#set-cookie" start="&quot;" end="&quot;">sc(Set-Cookie)</Field>
    <Field term="urn:vocab:clickstream#session-group" start="&quot;" end="&quot;">x-session-group</Field>
    <Field term="urn:vocab:clickstream#session-id" start="&quot;" end="&quot;">x-session-id</Field>
    <Field term="urn:vocab:clickstream#cookie-id" start="&quot;" end="&quot;">x-cookie-id</Field>
    <Field term="urn:vocab:clickstream#page-number" start="&quot;" end="&quot;">x-page-num</Field>
    <Field term="urn:vocab:clickstream#request-number" start="&quot;" end="&quot;">x-request-num</Field>
  </Codec>
