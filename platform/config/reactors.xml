<?xml version="1.0" encoding="UTF-8"?>
<PionConfig xmlns="http://purl.org/pion/config">
  <Reactor id="urn:uuid:153f6c40-cb78-11dc-8fa0-0019e3f89cd2">
    <Name>Only IE Requests</Name>
    <Comment>Filter events that are not requests from IE browsers</Comment>
    <Plugin>FilterReactor</Plugin>
    <Comparison>
    	<Term>urn:vocab:clf#useragent</Term>
    	<Type>contains</Type>
    	<Value>MSIE</Value>
    </Comparison>
  </Reactor>
  <Reactor id="urn:uuid:0cc21558-cf84-11dc-a9e0-0019e3f89cd2">
    <Name>Do Nothing</Name>
    <Comment>Filter that does nothing</Comment>
    <Plugin>FilterReactor</Plugin>
  </Reactor>
  <Connection>
    <From>urn:uuid:153f6c40-cb78-11dc-8fa0-0019e3f89cd2</From>
    <To>urn:uuid:0cc21558-cf84-11dc-a9e0-0019e3f89cd2</To>
  </Connection>
</PionConfig>
